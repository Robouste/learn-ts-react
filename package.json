		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-query": "^3.39.2",
		"react-router-dom": "^6.4.2",
		"react-scripts": "^5.0.1",
		"typescript": "^4.8.4",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@types/node-sass": "^4.11.3",
		"@types/react-redux": "^7.1.24",
